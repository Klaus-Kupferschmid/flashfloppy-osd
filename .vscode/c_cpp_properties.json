{
    "version": 4,
    "configurations": [
        {
            "name": "STM32F103C8T6",
            "includePath": [
                "${workspaceFolder}/inc/**",
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "STM32F10X_MD",
                "USE_STDPERIPH_DRIVER",
                "F103C8T6",
                "FW_VER=\"1.9\"",
                "__GNUC__"
            ],
            "compilerPath": "arm-none-eabi-gcc",
            "cStandard": "gnu99",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m3",
                "-mthumb",
                "-mfloat-abi=soft",
                "-mlittle-endian"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/inc",
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
